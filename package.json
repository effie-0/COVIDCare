{
  "name": "COVIDCare",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel src/ --out-dir dist/",
    "coverage": "export NODE_ENV='test' && jest --verbose test/ --bail --runInBand --coverage",
    "lint": "eslint 'src/' && eslint 'public/javascripts/'",
    "start": "npm run build && node dist/bin/www",
    "serve": "node dist/bin/www",
    "test": "export NODE_ENV='test' && jest --verbose test/"
  },
  "dependencies": {
    "bulma": "^0.9.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "http-errors": "~1.6.3",
    "mongoose": "^5.10.15",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
